<#student_0> a rr:TriplesMap;

	rml:logicalSource [
		a rml:LogicalSource;
		rml:source "student.json";
		rml:referenceFormulation ql:JSONPath;
		rml:iterator "$.students[*]";
	];
	rr:subjectMap [
		a rr:SubjectMap;
		rr:template "http://example.com/resource/student_{ID}";
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant foaf:name;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rml:reference "Name";
		];
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant <http://example.com/ontology/practises>;
		];
		rr:objectMap [
			a rr:RefObjectMap;
			rr:parentTriplesMap <#sport_0>;
			rr:joinCondition [
				rr:child "Sport";
				rr:parent "Name";
			];
		];
	].


<#sport_0> a rr:TriplesMap;

	rml:logicalSource [
		a rml:LogicalSource;
		rml:source "sport.json";
		rml:referenceFormulation ql:JSONPath;
		rml:iterator "$.sports[*]";
	];
	rr:subjectMap [
		a rr:SubjectMap;
		rr:template "http://example.com/resource/sport_{ID}";
	];
	rr:predicateObjectMap [
		rr:predicateMap [
			a rr:PredicateMap;
			rr:constant rdfs:label;
		];
		rr:objectMap [
			a rr:ObjectMap;
			rml:reference "Name";
		];
	].


